{
    "name": "BetterKabu",
    "description": "BetterKabu: Dark Mode, Tages-/Stunden-Hervorhebung & Stunden-Timer f√ºr Digikabu.de",
    "version": "1.6",
    "manifest_version": 3,
    "icons": {
        "16": "/icons/icon16.png",
        "32": "/icons/icon32.png",
        "48": "/icons/icon48.png",
        "128": "/icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/icons/icon16.png",
            "32": "/icons/icon32.png",
            "48": "/icons/icon48.png",
            "128": "/icons/icon128.png"
        }
    },
    "content_scripts": [{
        "matches": ["https://*.digikabu.de/*", "http://*.digikabu.de/*", "https://digikabu.de/*", "http://digikabu.de/*"],
        "js": ["init.js"],
        "css": ["init.css"]
    }]
}